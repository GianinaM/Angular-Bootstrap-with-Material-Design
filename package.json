{
  "name": "ng-mdb",
  "version": "5.1.1",
  "description": "Bootstrap Material Design for Angular",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "commit": "commit",
    "precommit": "lint-staged",
    "build": "ts-node ./build/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.1",
    "@angular/cli": "^1.6.5",
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/compiler-cli": "^5.2.1",
    "@angular/core": "^5.2.1",
    "@angular/forms": "^5.2.1",
    "@angular/http": "^5.2.1",
    "@angular/language-service": "^5.2.1",
    "@angular/platform-browser": "^5.2.1",
    "@angular/platform-browser-dynamic": "^5.2.1",
    "@angular/router": "^5.2.1",
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "@commitlint/prompt-cli": "^6.0.2",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "^22.0.1",
    "@types/lodash": "^4.14.92",
    "@types/node": "~9.3.0",
    "@types/ora": "^1.3.1",
    "@types/prettier": "^1.9.0",
    "@types/rimraf": "^2.0.2",
    "chalk": "^2.3.0",
    "codelyzer": "~4.1.0",
    "conventional-changelog-cli": "^1.3.5",
    "core-js": "^2.4.1",
    "cpy-cli": "^1.0.1",
    "font-awesome": "4.7.x",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "hammerjs": "^2.0.8",
    "husky": "^0.14.3",
    "jasmine-core": "~2.9.1",
    "jasmine-marbles": "^0.2.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.0.6",
    "jest-preset-angular": "^5.0.0",
    "lint-staged": "^6.0.0",
    "lodash": "^4.17.4",
    "ncp": "^2.0.0",
    "ora": "^1.3.0",
    "prettier": "^1.10.2",
    "pretty-format": "^22.1.0",
    "protractor": "~5.2.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.1",
    "rxjs": "^5.5.2",
    "sorcery": "^0.10.0",
    "ts-jest": "^22.0.1",
    "ts-node": "~4.1.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.6.2",
    "uglify-js": "^3.3.8",
    "zone.js": "^0.8.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mdbootstrap/Angular-Bootstrap-with-Material-Design"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "lint-staged": {
    "*.ts": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jest.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "example-app/tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/example-app/**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "html",
      "json"
    ],
    "mapCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/modules/*.*/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx)"
    ],
    "moduleNameMapper": {
      "angular-bootstrap-md": "<rootDir>/src"
    },
    "modulePathIgnorePatterns": [
      "dist"
    ]
  }
}
